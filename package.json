{
  "name": "bradpitt",
  "version": "0.0.1",
  "description": "Set of javascript modules that do cool stuff",
  "main": "dist/bradpitt.js",
  "scripts": {
    "build": "mkdir -p dist && browserify index.js --standalone bradpitt > dist/bradpitt.js"
  },
  "browserify": {
    "debug": true,
    "cache": {},
    "packageCache": {},
    "extension": [
      "js"
    ],
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/the-couch/bradpitt.git"
  },
  "keywords": [
    "javascript",
    "animations",
    "es6"
  ],
  "author": "iamkevingreen <meetus@thecouch.nyc> (http://thecouch.nyc)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the-couch/bradpitt/issues"
  },
  "homepage": "https://github.com/the-couch/bradpitt#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "bundle-size": "^0.7.0",
    "minifyify": "^7.3.3",
    "query-string": "^4.2.3",
    "raf": "^3.3.0",
    "watchify": "^3.7.0"
  }
}
